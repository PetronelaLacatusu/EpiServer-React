@model FaqTabs

<div class="hbhBrick faqs">
    <ul class="FAQTabs equalHeights">
        @if (Model.Faqs.Any())
        {
            foreach (var tab in Model.Faqs.FilteredItemsOfType<FaqsBlock>())
            {
                var head = tab.Heading ?? string.Empty;
                <li class="">@head</li>
            }
        }
    </ul>

    @foreach (var item in Model.Faqs.FilteredItemsOfType<FaqsBlock>())
    {
        Html.RenderContentData(item, true, GlobalSettings.RenderingTags.FaqTabsItem);
    }

</div>
<script>
    $(document).ready(function () {
        var config = '/static/js/ccframework/config.js';
        require([config], function () {
            require(['faqs', 'calc']);
        });
    });
</script>
